<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="order.MainPage">

    <StackLayout>
        <Entry x:Name="num1"/>
        <Entry x:Name="num2"/>
        <Entry x:Name="num3"/>
        <Entry x:Name="num4"/>
        <Entry x:Name="num5"/>
        <StackLayout>
            <RadioButton Content="Ascending" x:Name="asc"/>
            <RadioButton Content="Decending" x:Name="dec"/>
        </StackLayout>
        <Button Clicked="Button_Clicked"/>
    </StackLayout>

</ContentPage>
